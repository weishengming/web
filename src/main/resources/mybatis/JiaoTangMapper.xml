<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.weishengming.dao.mapper.JiaoTangMapper">
	<sql id="all_columns">
		ID,CREATEDATE,UPDATEDATE,VERSION,AREA1ID,AREA1NAME,AREA2ID,AREA2NAME,AREA3ID,AREA3NAME,AREAID,AREANAME,BEIZHU,DIXIONGZIMEIID,DIXIONGZIMEIXINGMING,LEIXING,LEIXINGSTRING,NAME,NICKNAME,OPENID,XIANGXIDIZHI
	</sql>
	<sql id="where">
		<where>
			<if test="id != null">
				and ID = #{id}
			</if>
			<if test="createDate != null">
				and CREATEDATE = #{createDate}
			</if>
			<if test="updateDate != null">
				and UPDATEDATE = #{updateDate}
			</if>
			<if test="version != null">
				and VERSION = #{version}
			</if>
			<if test="area1Id != null">
				and AREA1ID = #{area1Id}
			</if>
			<if test="area1Name != null">
				and AREA1NAME = #{area1Name}
			</if>
			<if test="area2Id != null">
				and AREA2ID = #{area2Id}
			</if>
			<if test="area2Name != null">
				and AREA2NAME = #{area2Name}
			</if>
			<if test="area3Id != null">
				and AREA3ID = #{area3Id}
			</if>
			<if test="area3Name != null">
				and AREA3NAME = #{area3Name}
			</if>
			<if test="areaId != null">
				and AREAID = #{areaId}
			</if>
			<if test="areaName != null">
				and AREANAME = #{areaName}
			</if>
			<if test="beizhu != null">
				and BEIZHU = #{beizhu}
			</if>
			<if test="dixiongzimeiid != null">
				and DIXIONGZIMEIID = #{dixiongzimeiid}
			</if>
			<if test="dixiongzimeixingming != null">
				and DIXIONGZIMEIXINGMING = #{dixiongzimeixingming}
			</if>
			<if test="leixing != null">
				and LEIXING = #{leixing}
			</if>
			<if test="leixingstring != null">
				and LEIXINGSTRING = #{leixingstring}
			</if>
			<if test="name != null">
				and NAME = #{name}
			</if>
			<if test="nickname != null">
				and NICKNAME = #{nickname}
			</if>
			<if test="openID != null">
				and OPENID = #{openID}
			</if>
			<if test="xiangxidizhi != null">
				and XIANGXIDIZHI = #{xiangxidizhi}
			</if>
		</where>
	</sql>
	<sql id="orderById"> ORDER BY ID DESC </sql>
	<insert id="insert" useGeneratedKeys="true" keyProperty="id"
		parameterType="JiaoTangDO">
		INSERT INTO jiaotang (
		<include refid="all_columns" />
		) VALUES
		(#{id},#{createDate},#{updateDate},#{version},#{area1Id},#{area1Name},#{area2Id},#{area2Name},#{area3Id},#{area3Name},#{areaId},#{areaName},#{beizhu},#{dixiongzimeiid},#{dixiongzimeixingming},#{leixing},#{leixingstring},#{name},#{nickname},#{openID},#{xiangxidizhi})
	</insert>
	<update id="update" parameterType="JiaoTangDO">
		UPDATE jiaotang SET
                    <![CDATA[ ID = #{id}]]>,   
                    <![CDATA[ CREATEDATE = #{createDate}]]>,   
                    <![CDATA[ UPDATEDATE = #{updateDate}]]>,   
                    <![CDATA[ VERSION = #{version}]]>,   
                    <![CDATA[ AREA1ID = #{area1Id}]]>,   
                    <![CDATA[ AREA1NAME = #{area1Name}]]>,   
                    <![CDATA[ AREA2ID = #{area2Id}]]>,   
                    <![CDATA[ AREA2NAME = #{area2Name}]]>,   
                    <![CDATA[ AREA3ID = #{area3Id}]]>,   
                    <![CDATA[ AREA3NAME = #{area3Name}]]>,   
                    <![CDATA[ AREAID = #{areaId}]]>,   
                    <![CDATA[ AREANAME = #{areaName}]]>,   
                    <![CDATA[ BEIZHU = #{beizhu}]]>,   
                    <![CDATA[ DIXIONGZIMEIID = #{dixiongzimeiid}]]>,   
                    <![CDATA[ DIXIONGZIMEIXINGMING = #{dixiongzimeixingming}]]>,   
                    <![CDATA[ LEIXING = #{leixing}]]>,   
                    <![CDATA[ LEIXINGSTRING = #{leixingstring}]]>,   
                    <![CDATA[ NAME = #{name}]]>,   
                    <![CDATA[ NICKNAME = #{nickname}]]>,   
                    <![CDATA[ OPENID = #{openID}]]>,   
                    <![CDATA[ XIANGXIDIZHI = #{xiangxidizhi}]]>,
		version = version+1 WHERE
		id=#{id} and version = #{version}
	</update>
	<select id="findOne" parameterType="long" resultType="JiaoTangDO">
		SELECT
		<include refid="all_columns" />
		FROM jiaotang WHERE ID=#{id}
	</select>
	<select id="findAll" resultType="JiaoTangDO">
		SELECT
		<include refid="all_columns" />
		FROM jiaotang
	</select>
	<delete id="delete" parameterType="long">
		DELETE FROM jiaotang WHERE id=#{id}
	</delete>
	<delete id="deleteByIds">
		DELETE FROM jiaotang
		<where>
			ID in
			<foreach item="item" index="index" collection="list" open="("
				separator="," close=")">
				#{item}
			</foreach>
		</where>
	</delete>

	<select id="findList" resultType="JiaoTangDO">
		SELECT
		<include refid="all_columns" />
		FROM jiaotang
		<include refid="where" />
		<include refid="orderById" />
		LIMIT #{offset}, #{size}
	</select>
	<select id="getByIds" parameterType="list" resultType="JiaoTangDO">
		SELECT
		<include refid="all_columns" />
		FROM jiaotang
		<where>
			ID in
			<foreach item="item" index="index" collection="list" open="("
				separator="," close=")">
				#{item}
			</foreach>
		</where>
	</select>
	<select id="countList" resultType="long">
		SELECT count(1) FROM jiaotang
		<include refid="where" />
		<include refid="orderById" />
	</select>
	<select id="count" resultType="long">
		SELECT count(1) FROM jiaotang
		<include refid="orderById" />
	</select>
</mapper>
