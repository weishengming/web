<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.weishengming.dao.mapper.JTDXZMMapper">
	<sql id="all_columns">
		ID,CREATEDATE,UPDATEDATE,VERSION,DIXIONGZIMEIID,DIXIONGZIMEIXINGMING,JIAOTANGID,JIAOTANGMINGZI,NICKNAME,OPENID
	</sql>
	<sql id="where">
		<where>
			<if test="id != null">
				and ID = #{id}
			</if>
			<if test="createDate != null">
				and CREATEDATE = #{createDate}
			</if>
			<if test="updateDate != null">
				and UPDATEDATE = #{updateDate}
			</if>
			<if test="version != null">
				and VERSION = #{version}
			</if>
			<if test="dixiongzimeiid != null">
				and DIXIONGZIMEIID = #{dixiongzimeiid}
			</if>
			<if test="dixiongzimeixingming != null">
				and DIXIONGZIMEIXINGMING = #{dixiongzimeixingming}
			</if>
			<if test="jiaotangid != null">
				and JIAOTANGID = #{jiaotangid}
			</if>
			<if test="jiaotangmingzi != null">
				and JIAOTANGMINGZI = #{jiaotangmingzi}
			</if>
			<if test="nickname != null">
				and NICKNAME = #{nickname}
			</if>
			<if test="openID != null">
				and OPENID = #{openID}
			</if>
		</where>
	</sql>
	<sql id="orderById"> ORDER BY ID DESC </sql>
	<insert id="insert" useGeneratedKeys="true" keyProperty="id"
		parameterType="JTDXZMDO">
		INSERT INTO jtdxzm (
		<include refid="all_columns" />
		) VALUES
		(#{id},#{createDate},#{updateDate},#{version},#{dixiongzimeiid},#{dixiongzimeixingming},#{jiaotangid},#{jiaotangmingzi},#{nickname},#{openID})
	</insert>
	<update id="update" parameterType="JTDXZMDO">
		UPDATE jtdxzm SET
                    <![CDATA[ ID = #{id}]]>,   
                    <![CDATA[ CREATEDATE = #{createDate}]]>,   
                    <![CDATA[ UPDATEDATE = #{updateDate}]]>,   
                    <![CDATA[ VERSION = #{version}]]>,   
                    <![CDATA[ DIXIONGZIMEIID = #{dixiongzimeiid}]]>,   
                    <![CDATA[ DIXIONGZIMEIXINGMING = #{dixiongzimeixingming}]]>,   
                    <![CDATA[ JIAOTANGID = #{jiaotangid}]]>,   
                    <![CDATA[ JIAOTANGMINGZI = #{jiaotangmingzi}]]>,   
                    <![CDATA[ NICKNAME = #{nickname}]]>,   
                    <![CDATA[ OPENID = #{openID}]]>,
		version = version+1 WHERE
		id=#{id} and version = #{version}
	</update>
	<select id="findOne" parameterType="long" resultType="JTDXZMDO">
		SELECT
		<include refid="all_columns" />
		FROM jtdxzm WHERE ID=#{id}
	</select>
	<select id="findAll" resultType="JTDXZMDO">
		SELECT
		<include refid="all_columns" />
		FROM jtdxzm
	</select>
	<delete id="delete" parameterType="long">
		DELETE FROM jtdxzm WHERE id=#{id}
	</delete>
	<delete id="deleteByIds">
		DELETE FROM jtdxzm
		<where>
			ID in
			<foreach item="item" index="index" collection="list" open="("
				separator="," close=")">
				#{item}
			</foreach>
		</where>
	</delete>

	<select id="findList" resultType="JTDXZMDO">
		SELECT
		<include refid="all_columns" />
		FROM jtdxzm
		<include refid="where" />
		<include refid="orderById" />
		LIMIT #{offset}, #{size}
	</select>
	<select id="getByIds" parameterType="list" resultType="JTDXZMDO">
		SELECT
		<include refid="all_columns" />
		FROM jtdxzm
		<where>
			ID in
			<foreach item="item" index="index" collection="list" open="("
				separator="," close=")">
				#{item}
			</foreach>
		</where>
	</select>
	<select id="countList" resultType="long">
		SELECT count(1) FROM jtdxzm
		<include refid="where" />
		<include refid="orderById" />
	</select>
	<select id="count" resultType="long">
		SELECT count(1) FROM jtdxzm
		<include refid="orderById" />
	</select>
</mapper>
